                <property
                  dataType="System.String"
                  description="Der auszuführende SQL-Befehl."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">;WITH TA AS
(
  SELECT [Tax Jurisdiction Code]
       , [Tax Group Code]
       , MAX([Effective Date]) [Effective Date]
    FROM [DynNavHRS].[dbo].[HRS$Tax Detail] WITH (NOLOCK)
GROUP BY [Tax Jurisdiction Code]
       , [Tax Group Code]
), HD AS 
(
  SELECT TD.[Tax Group Code]
       , SUM(CASE WHEN TD.[Tax Jurisdiction Code] IN ('VAT','SERVICETAX') THEN TD.[Maximum Amount_Qty_] ELSE 0 END) [Roomprice tax %]
    FROM [DynNavHRS].[dbo].[HRS$Tax Detail] TD WITH (NOLOCK)
    JOIN [DynNavHRS].[dbo].[HRS$Tax Group] TG WITH (NOLOCK)
      ON TG.[Code] = TD.[Tax Group Code]	
    JOIN [DynNavHRS].[dbo].[HRS$Customer] CU WITH (NOLOCK)
      ON CU.[No_] = TD.[Tax Group Code]
    JOIN TA
      ON TA.[Tax Jurisdiction Code] = TD.[Tax Jurisdiction Code]
     AND TA.[Tax Group Code] = TD.[Tax Group Code]
     AND TA.[Effective Date] = TD.[Effective Date]
   WHERE TG.[Use Hotelstamm] = 1
GROUP BY TD.[Tax Group Code]
  HAVING SUM(CASE WHEN TD.[Tax Jurisdiction Code] IN ('VAT','SERVICETAX') THEN TD.[Maximum Amount_Qty_] ELSE 0 END)&gt;0
)
 SELECT NULL [Country Code]
      , [Tax Group Code] [Hotel No]
	  , NULL [Post Code]
	  , NULL [Room price limit]
	  , [Roomprice tax %]
	  , NULL [Breakfast tax %]
      , NULL [Deduction type]
	  , NULL [Deductible amount]
   FROM HD
UNION
SELECT NULL [Country Code]
     , NULL [Hotel No]
	 , [Code] [Post Code]
	 , NULL
	 , NULL
	 , NULL
	 , DT.String [Deduction type]
	 , [Deductible Amount]
  FROM DynNavHRS.dbo.[HRS$Post Code] PC WITH (NOLOCK)
  JOIN DynNavHRS.dbo.Split('none,before tax,after tax,per person,per roomnight,per stay',',') DT
    ON DT.[Index] = [Deduction Type]+1
 WHERE [Deduction Type]&gt;0
UNION
 SELECT [Country] [Country Code]
      , NULL [Hotel No]
	  , NULL [Post Code]
	  , NULL [Room price limit]
      , [VAT in %] * [Use VAT] + [Service Tax] * [Use Service Tax] [Roomprice tax %]
      , [Breakfast Tax] * [Use Breakfast Tax] [Breakfast tax %]
      , NULL [Deduction type]
	  , NULL [Deductible amount]
  FROM [DynNavHRS].[dbo].[HRS$Foreign Tax] WITH (NOLOCK)
 WHERE [Country]&lt;&gt;0 AND [Country]&lt;&gt;99999

ORDER BY 1,3,2</property>
